{
  "manifest_version": 3,
  "name": "Debug-GPT",
  "description": "Your AI-Powered Debugging Assistant.",
  "version": "1.0.1",
  "permissions": [
    "debugger",
    "activeTab",
    "storage"
  ],
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "devtools_page": "devtools.html",
  "action": {},
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
